int data;

void setup() {
  Serial.begin(9600);
  pinMode(LED_BUILTIN, OUTPUT);
  digitalWrite (LED_BUILTIN, LOW);

  Serial.println("Welcome to the MTR+SWARM");
}

void loop() {
while (Serial.available())(
  data = Serial.read());

  if (data == '1')
digitalWrite (LED_BUILTIN, HIGH);

  else if (data == '2')
digitalWrite (LED_BUILTIN, LOW);

}
